# Database Configuration
DATABASE_URL=mysql+pymysql://traffic_user:your_password_here@your_host_here:3306/traffic_management

# JWT Configuration
SECRET_KEY=your-secret-key-here-generate-a-secure-one
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# CORS Configuration (for production)
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:5173,https://your-frontend-domain.vercel.app

# Email Configuration (if using email features)
SMTP_SERVER=smtp.gmail.com
SMTP_PORT=587
SMTP_USERNAME=your-email@gmail.com
SMTP_PASSWORD=your-app-password

# Weather API (if using weather features)
OPENWEATHER_API_KEY=your-openweather-api-key

# External APIs (if needed)
TOMTOM_API_KEY=your-tomtom-api-key

# Railway Database Variables (these will be auto-generated by Railway)
MYSQL_ROOT_PASSWORD=
MYSQL_PASSWORD=
MYSQL_HOST=
MYSQL_PORT=3306
MYSQL_DATABASE=traffic_management
